{
  "name": "law2x-basic-postgress",
  "version": "1.0.0",
  "dependencies": {
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "@cap-js/postgres": "^2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-law2x-people-app": "cds watch --open law2xpeopleapp/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy law2x-basic-postgress --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/law2x-people-app"
  ]
}
